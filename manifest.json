{
  "name": "Search Faces",
  "version": "1.0",
  "description": "Search Faces",
  "permissions": ["storage", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>", "http://localhost:11434/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/index.js"],
      "css": ["src/content/style.css"]
    }
  ],
  "background": {
    "service_worker": "src/background/index.js"
  },
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "src/assets/images/icon_famous16.png",
      "32": "src/assets/images/icon_famous32.png",
      "48": "src/assets/images/icon_famous48.png",
      "128": "src/assets/images/icon_famous128.png"
    }
  },
  "icons": {
    "16": "src/assets/images/icon_famous16.png",
    "32": "src/assets/images/icon_famous32.png",
    "48": "src/assets/images/icon_famous48.png",
    "128": "src/assets/images/icon_famous128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["src/assets/images/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "manifest_version": 3
}
